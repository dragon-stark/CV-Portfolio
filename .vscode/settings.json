{
  "prettier.enableDebugLogs": true,
  "rainbow_csv.enable_context_menu_head": true,
  "rainbow_csv.enable_context_menu_tail": true,
  "sqltools.connectionExplorer.groupConnected": true,
  "sqltools.queryParams.enableReplace": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": false,
  "javascript.referencesCodeLens.enabled": false,
  "prettyxml.settings.formatOnSave": true,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.wordWrap": "on",
  "editor.autoIndent": "brackets",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.definitionLinkOpensInPeek": true,
  "editor.guides.bracketPairs": "active",
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.useSplitJSON": true,
  "window.autoDetectColorScheme": true,
  "git.mergeEditor": true,
  "html.format.indentInnerHtml": true,
  "prettier.withNodeModules": true,
  "rainbow_csv.csv_lint_detect_trailing_spaces": true,
  "editor.defaultFormatter": "SimonSiefke.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "diffEditor.codeLens": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable"
}
